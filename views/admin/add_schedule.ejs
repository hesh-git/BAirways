<form action="/admin/add_schedule" method="POST">
    <div class="form-group row mt-5">
        <label for="flightNo" class="col-sm-2 col-form-label">Flight No</label>
         
        <select name="FlightNo" id="flightNo" class="col-sm-10 custom-select">
            <% for(let i = 0; i < flightNoList.length; i++){ %>
                <option><%= flightNoList[i] %> </option>
            <% } %> 
        </select>
    </div>

    <div class="form-group row mt-4">
        <label for="aircraftID" class="col-sm-2 col-form-label">Aircraft ID</label>
        <select name="AircraftID" id="aircraftID" class="col-sm-10 custom-select">
            <% for(let i = 0; i < aircraftIDList.length; i++) { %> 
                <option><%= aircraftIDList[i] %> </option>
            <% } %> 
        </select>
    </div>

    <div class="form-group row mt-4">
        <label for="startTime" class="col-sm-2 col-form-label">Start Time</label>
        <div class="col-sm-10">
            <input type="datetime-local" name="StartTime" class="form-control" id="startTime" required>
        </div>
    </div>

    <div class="form-group row mt-4">
        <label for="endTime" class="col-sm-2 col-form-label">End Time</label>
        <div class="col-sm-10">
            <input type="datetime-local" name="EndTime" class="form-control" id="endTime" required>
        </div>
    </div>

    <button type="submit" class="mt-5 link link-btn btn btn-outline-primary btn-lg btn-block">Add</button>
</form>